{
    "projectId": "68c457dd00313b94f0a8",
    "projectName": "Comicskaan",
    "sites": [
        {
            "$id": "comicskaan-site",
            "name": "Comicskaan",
            "buildCommand": "npm run build",
            "outputDir": "dist",
            "rootDir": "."
        }
    ],
    "functions": [
        {
            "$id": "generate-comic",
            "name": "generate-comic",
            "runtime": "node-18.0",
            "execute": [
                "any"
            ],
            "events": [],
            "schedule": "",
            "timeout": 60,
            "enabled": true,
            "logging": true,
            "entrypoint": "src/index.js",
            "commands": "npm install",
            "ignore": [
                "node_modules",
                ".npm"
            ],
            "path": "functions/generate-comic",
            "vars": {
                "GEMINI_API_KEY": ""
            }
        }
    ]
}